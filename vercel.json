{
  "version": 2,
  "name": "talent-show-form",
  "builds": [
    {
      "src": "vercel-server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/static.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "vercel-server.js"
    },
    {
      "src": "/public/(.*)",
      "dest": "api/static.js"
    },
    {
      "src": "/styles.css",
      "dest": "api/static.js"
    },
    {
      "src": "/script.js",
      "dest": "api/static.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/static.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
